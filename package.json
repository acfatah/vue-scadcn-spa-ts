{
  "name": "vue-scadcn-ts",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint",
    "lint:changed": "eslint --no-warn-ignored $(git diff --name-only --diff-filter=d HEAD)",
    "lint:fix": "npm run lint:changed -- --fix",
    "lint:inspect": "npx eslint --inspect-config",
    "precommit": "npm run lint:fix",
    "check-updates": "npx npm-check-updates --target semver",
    "update-deps": "npm run check-updates -- -u && npm install && npm audit fix"
  },
  "dependencies": {
    "@radix-icons/vue": "^1.0.0",
    "@unovis/ts": "^1.4.5",
    "@unovis/vue": "^1.4.5",
    "@vee-validate/zod": "^4.14.7",
    "@vueuse/core": "^12.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-vue": "^8.5.1",
    "radix-vue": "^1.9.11",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "v-calendar": "^3.1.2",
    "vaul-vue": "^0.2.0",
    "vee-validate": "^4.14.7",
    "vue": "^3.5.13",
    "vue-sonner": "^1.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.11.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.16.0",
    "eslint-plugin-format": "^0.1.3",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "postcss": "^8.4.49",
    "simple-git-hooks": "^2.11.1",
    "tailwindcss": "^3.4.16",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vue-tsc": "^2.1.10"
  }
}
